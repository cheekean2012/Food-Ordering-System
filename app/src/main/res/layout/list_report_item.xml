<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/report_item_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/timeStamp_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1700685678"
        android:layout_margin="8dp"
        android:textStyle="bold"
        android:textSize="18sp"
        android:paddingRight="8dp"
        android:paddingVertical="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/table_number_textview"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/table_number_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:layout_margin="8dp"
        android:textAlignment="center"
        android:textSize="18sp"
        android:text="7"
        android:padding="8dp"
        app:layout_constraintStart_toEndOf="@id/timeStamp_textView"
        app:layout_constraintTop_toTopOf="@id/timeStamp_textView"
        app:layout_constraintBottom_toBottomOf="@id/timeStamp_textView"
        app:layout_constraintEnd_toStartOf="@id/total_price_textview"/>


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/total_price_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:text="RM177.00"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/timeStamp_textView"
        app:layout_constraintStart_toEndOf="@id/table_number_textview"
        app:layout_constraintEnd_toStartOf="@id/status_textview"
        app:layout_constraintTop_toTopOf="@id/timeStamp_textView" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/status_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginVertical="8dp"
        android:layout_marginHorizontal="8dp"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        android:text="COMPLETED"
        android:padding="8dp"
        app:layout_constraintStart_toEndOf="@id/total_price_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/timeStamp_textView"
        app:layout_constraintBottom_toBottomOf="@id/timeStamp_textView"/>

    <com.google.android.material.divider.MaterialDivider
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>